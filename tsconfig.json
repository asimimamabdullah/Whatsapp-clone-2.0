{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"allowJs": true,
		"esModuleInterop": true,
		"jsx": "react-native",
		"lib": ["DOM", "ESNext", "dom", "dom.iterable"],
		"moduleResolution": "node",
		"noEmit": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"target": "ESNext",
		"strict": true
	}
}
